<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.travelsky.ssm.mapper.UserMapper">
	<!-- 新增用户 -->
	<insert id="insertUser" parameterType="pojo">
		INSERT INTO user (
		username,
		password,
		id
		)
		VALUES
		(
		#{username},
		#{password},
		#{id}
		)
	</insert>
</mapper>
